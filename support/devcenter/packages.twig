<table>
	<thead>
		<tr>
			<th>{{ heading }}&nbsp;/&nbsp;series</th>
		{%~ for stack in stacks %}
			<th><a href="/articles/heroku-{{ stack }}-stack"><code>heroku-{{ stack }}</code></a></th>
		{%~ endfor %}
		</tr>
	</thead>
	<tbody>
	{%~ for package in packages %}
		<tr
		{%- if attribute(eol, package.series) is defined %} style="
			{%- if attribute(eol, package.series) == "eol" -%}
				background-color: rgba(194, 66, 0, 0.1); color: #C24200
			{%- else -%}
				background-color: #fff3c5; color: #927200
			{%- endif -%}
		"
		{%- endif -%}
		>
			<td style="white-space:nowrap">{{ package.name }} {{ package.series }}</td>
		{%~ for stack in stacks %}
			<td>{% if attribute(package, stack) %}{{ attribute(package, stack) }}{% else %}-{% endif %}</td>
		{%~ endfor %}
		</tr>
	{%~ endfor %}
	</tbody>
</table>
